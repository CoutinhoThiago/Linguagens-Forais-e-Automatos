Q = {'q0', 'q1', 'q2', 'q3'}
Sigma = {'0','1'}
delta = {('q0','0'):'q0',
         ('q0','1'):'q1',
         ('q1','0'):'q1',
         ('q1','1'):'q2',
         ('q2','0'):'q2',
         ('q2','1'):'q3',
         ('q3','0'):'q3'}
q0 = 'q0'
F = {'q0', 'q1', 'q2', 'q3'}
 
M = AFD(Q,Sigma,delta,q0,F)
M.visualizar()
