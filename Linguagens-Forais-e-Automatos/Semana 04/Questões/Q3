Q = {'q0','a','b','c','f'}
Sigma = {'a','b','c'}
delta = {('q0','a'):'a',
         ('q0','b'):'b',
         ('q0','c'):'c',
         ('a','a'):'a',
         ('a','b'):{'a','f'},
         ('a','c'):{'a','f'},
         ('b','a'):{'b','f'},
         ('b','b'):'b',
         ('b','c'):{'b','f'},
         ('c','a'):{'c','f'},
         ('c','b'):{'c','f'},
         ('c','c'):{'c'}}

q0 = 'q0'
F = {'f','q0'}
 
M = AFN(Q,Sigma,delta,q0,F)
M.visualizar()
